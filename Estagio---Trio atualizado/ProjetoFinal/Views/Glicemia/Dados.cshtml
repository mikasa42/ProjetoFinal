@model GlicemiaModel

<h3>Meu Nivel glicemico</h3>>

<div class="container">
    <form asp-controller="Glicemia" asp-action="Dados">
        <div class="form-group">
            <label>Data  (Início):</label>
            <input type="date" asp-for="data_cadastro" class="form-control">
        </div>
        <div class="form-group">
            <label>Cpf</label>
            <select asp-for="Cpf" class="form-control">
                @{
                    foreach (var item in (List<GlicemiaModel>)ViewBag.ListaGlicemia)
                    {
                    <option value="@item.Cpf.ToString()">@item.Nome.ToString()</option>

                    }
                }
            </select>
        </div>
        <button type="submit" class="btn btn-block btn-success">Pesquisar</button>
    </form>
</div>
<br />
<table class="table table-bordered">
    <thead>
        <tr>
            <th>#</th>
            <th>CPR</th>
            <th>NIVEL GLICEMICO</th>
            <th>DATA</th>
            
        </tr>
    </thead>





<script>
    function Editar(id) {
        window.location.href = "../Glicemia/Registrar/" + id;
    }
</script>